#include<stdio.h>
int main() {
    int arr[5] = {3, 6, 8, 2, 7}; 
    int i, key;
    printf("Enter Element to search: ");
    scanf("%d", &key);
    for (i = 0; i < 5; i++) { 
        if (arr[i] == key) {
            printf("Element found at %d", i);
            return 0; 
        }
    }
    printf("Element not found");
    return 0;
}
